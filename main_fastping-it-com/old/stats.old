    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            background: linear-gradient(135deg, #0f0f23 0%, #1a1a3a 50%, #2d2d5f 100%);
            color: #ffffff;
            font-family: 'Inter', -apple-system, BlinkMacSystemFont, sans-serif;
            min-height: 100vh;
            overflow-x: hidden;
        }

        .hero-section {
            position: relative;
            padding: 4rem 2rem;
            text-align: center;
            background: radial-gradient(circle at center, rgba(64, 224, 255, 0.1) 0%, transparent 70%);
        }

        .hero-title {
            font-size: clamp(2.5rem, 5vw, 4.5rem);
            font-weight: 800;
            background: linear-gradient(135deg, #40e0ff, #4facfe, #00f2fe);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
            margin-bottom: 1rem;
            text-shadow: 0 0 30px rgba(64, 224, 255, 0.3);
        }

        .hero-subtitle {
            font-size: 1.5rem;
            color: #b3b3ff;
            margin-bottom: 3rem;
            font-weight: 300;
        }

        .pulse-indicator {
            display: inline-block;
            width: 12px;
            height: 12px;
            background: #00ff88;
            border-radius: 50%;
            margin-right: 8px;
            animation: pulse 2s infinite;
            box-shadow: 0 0 10px #00ff88;
        }

        @keyframes pulse {
            0% { transform: scale(1); opacity: 1; }
            50% { transform: scale(1.2); opacity: 0.7; }
            100% { transform: scale(1); opacity: 1; }
        }

        .stats-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 2rem;
            padding: 2rem;
            max-width: 1400px;
            margin: 0 auto;
        }

        .stat-card {
            background: rgba(255, 255, 255, 0.05);
            backdrop-filter: blur(20px);
            border: 1px solid rgba(255, 255, 255, 0.1);
            border-radius: 20px;
            padding: 2rem;
            transition: all 0.3s ease;
            position: relative;
            overflow: hidden;
        }

        .stat-card:hover {
            transform: translateY(-5px);
            border-color: rgba(64, 224, 255, 0.5);
            box-shadow: 0 20px 40px rgba(64, 224, 255, 0.2);
        }

        .stat-card::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 3px;
            background: linear-gradient(90deg, #ff6b6b, #4ecdc4, #45b7d1, #96ceb4, #feca57);
            background-size: 300% 100%;
            animation: gradient 3s ease infinite;
        }

        @keyframes gradient {
            0% { background-position: 0% 50%; }
            50% { background-position: 100% 50%; }
            100% { background-position: 0% 50%; }
        }

        .stat-number {
            font-size: clamp(2.5rem, 4vw, 3.5rem);
            font-weight: 900;
            color: #40e0ff;
            display: block;
            margin-bottom: 0.5rem;
            text-shadow: 0 0 20px rgba(64, 224, 255, 0.4);
        }

        .stat-label {
            font-size: 1.1rem;
            color: #b3b3ff;
            font-weight: 500;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .stat-subtitle {
            font-size: 0.9rem;
            color: #888;
            margin-top: 0.5rem;
            font-style: italic;
        }

        .mega-stat {
            grid-column: 1 / -1;
            text-align: center;
            background: linear-gradient(135deg, rgba(64, 224, 255, 0.1), rgba(75, 172, 254, 0.1));
            border: 2px solid rgba(64, 224, 255, 0.3);
        }

        .mega-stat .stat-number {
            font-size: clamp(4rem, 8vw, 8rem);
            background: linear-gradient(135deg, #40e0ff, #4facfe, #00f2fe);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
        }

        .performance-section {
